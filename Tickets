mysql> CREATE TABLE tickets (     ticket_id INT PRIMARY KEY,     customer_name VARCHAR(100),     issue_type VARCHAR(50),
    ->     priority INT,                 -- 1 (Low), 2 (Medium), 3 (High)     status VARCHAR(20),          -- Open, Closed, Pending, Escalated     created_date DATE,     assigned_team VARCHAR(50),   -- Support Team names     location VARCHAR(50)         -- City or Branch location
    -> );

