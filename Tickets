mysql> SELECT
    ->     ticket_id,
    ->     priority,
    ->     CASE priority
    ->         WHEN 3 THEN 'High'
    ->         WHEN 2 THEN 'Medium'
    ->         WHEN 1 THEN 'Low'
    ->     END AS urgency,
    ->     status,
    ->     CASE
    ->         WHEN status IN ('Open', 'Escalated') THEN 'Active'
    ->         WHEN status = 'Closed' THEN 'Inactive'
    ->         ELSE 'Waiting'
    ->     END AS status_classification
